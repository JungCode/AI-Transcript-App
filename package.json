{
  "name": "source-base",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "check-types": "npx cross-env NODE_OPTIONS=--max-old-space-size=8192 tsc --noEmit --pretty",
